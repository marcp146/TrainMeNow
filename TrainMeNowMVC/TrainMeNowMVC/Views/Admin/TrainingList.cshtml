@model List<TrainMeNowMVC.Models.TrainingViewModel>
@{
    ViewBag.Title = "TrainingList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Training List</h2>


@foreach (var training in Model)
{
    <div class="table" >
        Id: @training.Id
        Name: @training.Name
        Trainer Id: @training.TrainerId
        Price: @training.Price
        Max Users: @training.MaxUsers
    </div>
}






