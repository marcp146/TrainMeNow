@model List<TrainMeNowMVC.Models.TrainingViewModel>
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

@foreach (var training in Model)
{   

    <div class="well">

        <h3>Training Id: @training.Id</h3> 
        <h3>Training Name: @training.Name</h3> 
        <h3>Trainer Id: @training.TrainerId</h3> 
        <h3>Training Price:@training.Price</h3> 
        <h3>Training MaxUsers:@training.MaxUsers</h3>  

       



    </div>
}
